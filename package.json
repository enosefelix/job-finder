{
  "name": "job-finder-be",
  "version": "1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build-commands": "npm install && yarn db.migration.run && yarn db.generate && yarn db.seed",
    "build-prod": "ts-node ./src/common/prisma/script-runner.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db.migration": "npx prisma migrate dev --create-only --schema=./src/common/prisma/schema.prisma",
    "db.migration.apply": "npx prisma migrate dev --schema=./src/common/prisma/schema.prisma && yarn db.generate",
    "db.migration.run": "npx prisma migrate deploy --schema=./src/common/prisma/schema.prisma && yarn db.generate",
    "db.generate": "npx prisma generate --schema=./src/common/prisma/schema.prisma",
    "db.seed": "ts-node src/common/prisma/seed.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.15.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.8",
    "@types/passport-jwt": "^3.0.8",
    "axios": "^1.4.0",
    "bcrypt": "^5.0.0",
    "buffer-to-stream": "^1.0.0",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.40.0",
    "crypto": "^1.0.1",
    "date-fns": "^2.30.0",
    "docx2pdf": "^0.0.4",
    "dotenv": "^16.0.3",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "nestjs-real-ip": "^2.2.0",
    "node-pandoc": "^0.3.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^4.15.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "ts-node": "^10.0.0",
    "tsx": "^3.12.7",
    "wide-align": "1.1.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.14",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "^20.2.5",
    "@types/request-ip": "^0.0.38",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
